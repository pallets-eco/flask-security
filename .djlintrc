{
    "ignore": "H005,H006,H017,H025,H030,H031",
    "extension": "html",
    "indent": "2",
    "profile": "jinja",
    "format_attribute_template_tags": "true",
    "max_line_length": 120,
    "max_attribute_length": 240,
    "blank_line_after_tag": "from,endmacro",
    "blank_line_before_tag": "block,extends"
}
